<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="nit.livetex.livetexsdktestapp" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="nit.livetex.livetexsdktestapp.MainApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="nit.livetex.livetexsdktestapp.ChatActivity"
            android:label="@string/title_activity_chat" >
        </activity>

        <service
            android:name="livetex.sdk.requests.ClientRequestService"
            android:exported="false" >
        </service>
        <service
            android:name="livetex.sdk.service.NotificationVisitorService"
            android:enabled="true"
            android:exported="false" >
        </service>

        <receiver
            android:name="livetex.sdk.service.NotificationVisitorReciever"
            android:enabled="true"
            android:exported="false" >
        </receiver>

        <activity
            android:name="nit.livetex.livetexsdktestapp.WelcomeActivity"
            android:label="@string/title_activity_welcome" >
        </activity>
        <activity
            android:name="nit.livetex.livetexsdktestapp.InitActivity"
            android:label="@string/title_activity_init"
            android:noHistory="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="nit.livetex.livetexsdktestapp.ClaimActivity"
            android:label="@string/title_activity_claim" >
        </activity>

    </application>

</manifest>
